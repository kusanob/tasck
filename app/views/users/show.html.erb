<% provide(:title, @user.name) %>

<h2>プロフィール</h2>
　<div class="row">
    <div class="col-md-6 col-md-offset-3">
      <table class="table table-bordered">
        <tr>
          <th class= "task_th">ID</th>
          <td><%= @user.id %></td>
        </tr>
        <tr>
          <th class= "task_th">名前</th>
          <td><%= @user.name %></td>
        </tr>
        <tr>
          <th class= "task_th">メールアドレス</th>
          <td><%= @user.email %></td>
        </tr>
        <tr>
          <th class= "task_th">作成日</th>
          <td><%= l @user.created_at, format: :long %></td>
        </tr>
        
      </table>
    </div>
  </div>